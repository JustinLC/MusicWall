<h1>All Songs</h1>
<hr/>
<% @songs.each do |song| %>
	<div class="song">
		<h3><a href="/songs/<%= song.id %>"><%= song.name %></a> <small>(by: <%= song.artist %>)</small></h3>
		<p>
			<% song.url %>
		</p>
	</div>
<% end %>
<hr/>
<h4><a href="/songs/new">Post a song... It's Free (for now)!</a></h4>